---
import Layout from "../layout/Layout.astro";

const title = "Resources";
const description =
  "Whether you're a beginner just starting out or an experienced professional looking to level up, these resources are designed to help you succeed.";
const permalink = Astro.url.href;
---

<Layout title={title} description={description} permalink={permalink}>
  <div class="container mx-auto px-4 py-10 md:py-20 min-h-screen">
    <h1 class="text-5xl font-bold max-w-lg mx-auto">Resources</h1>
    <div class="flex flex-col my-12 md:my-24 max-w-lg mx-auto">
      <p class="text-lg leading-normal mb-8">Leetcode:</p>
      <a href="https://leetcode.com/problemset/all/"
        >https://leetcode.com/problemset/all/</a
      >
    </div>
  </div>
</Layout>
